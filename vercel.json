{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/manager",
            "dest": "/api/manager.php"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}